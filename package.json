{
  "name": "igdownload",
  "version": "0.0.1",
  "engines": {
    "node": "12"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "dev:ssr": "ng run igdownload:serve-ssr",
    "serve:ssr": "node dist/igdownload/server/main.js",
    "build:ssr": "ng build --prod && ng run igdownload:server:production",
    "prerender": "ng run igdownload:prerender",
    "build:functions": "npm run --prefix functions build",
    "build:all": "npm run build:ssr && npm run build:functions",
    "start:heroku": "node dist/igdownload/server/main.js",
    "postinstall": "npm run build:ssr"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~10.0.14",
